<%= form_for(access) do |f| %>
  <% if access.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(access.errors.count, "error") %> prohibited this access from being saved:</h2>

      <ul>
      <% access.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :date %>
    <%= f.datetime_select :date %>
  </div>

  <div class="field">
    <%= f.label :nationality %>
    <%= f.text_area :nationality %>
  </div>

  <div class="field">
    <%= f.label :os %>
    <%= f.text_area :os %>
  </div>

  <div class="field">
    <%= f.label :uuid %>
    <%= f.text_area :uuid %>
  </div>

<div class="row">
      <%= f.label :"Beacons" %>
    <%= f.select  :beacon_id, Beacon.all.collect { |s| [s.name, s.id] }%>
    </div>


  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
